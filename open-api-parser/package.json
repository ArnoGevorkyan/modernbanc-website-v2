{
  "name": "@modernbanc/open-api-parser",
  "description": "Modernbanc's open-api parser",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "version": "0.0.1",
  "license": "MIT",
  "author": "C-J",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "produce-file": "ts-node ./src/produce-file.tsx",
    "produce-file-ontop": "ts-node ./src/produce-file-ontop.tsx",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "test": "jest --config jestconfig.json",
    "prepublishCheck": "yarn test && yarn lint"
  },
  "devDependencies": {
    "@types/jest": "28.1.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "jest": "^28.1.1",
    "openapi-types": "12.0.0",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.5",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "jsx-to-string": "^1.4.0",
    "lodash": "4.17.21",
    "pretty-format": "^28.1.1",
    "react-element-to-jsx-string": "^15.0.0",
    "react-test-renderer": "^18.2.0"
  }
}